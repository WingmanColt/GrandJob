@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0"> 

    <meta name="author" content="GrandJob">
    <meta name="description" content="@ViewData["Description"]">
    <meta name="keywords" content="@ViewData["Keywords"]">
    <meta charset="UTF-8">

    <title>GrandJob | @ViewData["Title"]</title>
    <!-- Favicon-->
    <link defer rel="icon" href="~/uploads/favicon/favicon.ico" type="image/x-icon" />

    <link rel="stylesheet" href="~/theme_superio/css/preloader.min.css" asp-append-version="true">

    <!--Core-->
    <link defer rel="preload" href="~/theme_superio/css/bootstrapGrid.min.css" as="style" onload="this.onload=null;this.rel='stylesheet'" asp-append-version="true">
    <link defer rel="preload"  rel="stylesheet" href="~/theme_superio/css/style.min.css" as="style" onload="this.onload=null;this.rel='stylesheet'" asp-append-version="true" />
    <link defer rel="preload"  rel="stylesheet" href="~/theme_superio/css/responsive.min.css" as="style" onload="this.onload=null;this.rel='stylesheet'" asp-append-version="true" />

    <script defer src="https://cdn.jsdelivr.net/npm/lazysizes@5.3.2/lazysizes.min.js"></script>
</head>

<body data-anm=".anm">

    @RenderBody()

  <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

  <script defer src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>
  <script defer src="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/js/dist/util.min.js"></script>
  <script defer src="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/js/dist/dropdown.min.js"></script>
  
  <script defer src="~/theme_superio/js/jquery.modal.min.js"></script>

  <script defer src="https://cdnjs.cloudflare.com/ajax/libs/fancybox/3.5.7/jquery.fancybox.min.js"></script>y
  <script defer src="~/theme_superio/js/mmenu.polyfills.min.js"></script>
  <script defer src="https://cdnjs.cloudflare.com/ajax/libs/mmenu-js/8.4.3/mmenu.min.js"></script>
  <script defer src="~/theme_superio/js/appear.min.js"></script>
  <script defer src="~/theme_superio/js/anm.min.js"></script>
  <script defer src="https://cdnjs.cloudflare.com/ajax/libs/ScrollMagic/2.0.8/ScrollMagic.min.js"></script>
  <script defer src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js"></script>
  <script defer src="https://cdnjs.cloudflare.com/ajax/libs/wow/1.1.2/wow.min.js"></script>

    @await RenderSectionAsync("Scripts", required: false)

    <script defer src="~/theme_superio/js/script.min.js"></script>
   
    @await Component.InvokeAsync("NToastNotify")

</body>
</html>
